  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@types/nodemailer": "^6.4.14",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "cron": "^3.1.6",
    "dotenv": "^16.4.5",
    "env-var": "^7.4.1",
    "mongoose": "^8.2.2",
    "nodemailer": "^6.9.12",
    "prisma": "^5.11.0"
  }
